<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--站点目录，为空时取当前目录-->
    <add key="DomainPath" value=""/>
    <!--要删除的值-->
    <add key="DelLikeName" value="sohu."/>
    <!--运行方法,0新建站点，1删除站点,2自动检测文件重启IIS-->
    <add key="IsRunType" value="0"/>
    <!--重启IIS时的指令文件，如果文件存在，则重启IIS，并且删除文件，如果不存在则不处理-->
    <add key="IisFileRestart" value="E:\temp\iis\iis.restart"/>
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
</configuration>