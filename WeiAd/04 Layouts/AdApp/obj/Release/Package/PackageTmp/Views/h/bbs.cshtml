        function getElementsClass(classnames) {
            var classobj = new Array();//定义数组

            var classint = 0;//定义数组的下标

            var tags = document.getElementsByTagName("*");//获取HTML的所有标签

            for (var i in tags) {//对标签进行遍历

                if (tags[i].nodeType == 1) {//判断节点类型

